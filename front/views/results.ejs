<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Python Execution Result</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
            background-color: #f9f9f9;
        }

        .container {
            padding: 20px;
            border: 1px solid #ddd;
            background: #fff;
            border-radius: 8px;
            box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);
            overflow-x: auto;
        }

        h1 {
            color: #333;
            text-align: center;
        }

        .card {
            margin: 20px 0;
            padding: 15px;
            border: 1px solid #ddd;
            border-radius: 8px;
            background-color: #fefefe;
            box-shadow: 0 1px 4px rgba(0, 0, 0, 0.1);
        }

        .card h2 {
            margin-top: 0;
            font-size: 1.2em;
            color: #555;
        }

        .card p {
            margin: 5px 0;
            color: #666;
        }

        .card a {
            color: #007bff;
            text-decoration: none;
        }

        .card a:hover {
            text-decoration: underline;
        }

        .error {
            color: red;
            font-weight: bold;
        }

        pre {
            background-color: #f4f4f4;
            padding: 10px;
            border-radius: 4px;
            white-space: pre-wrap;
            word-wrap: break-word;
        }
    </style>
</head>
<body>
    <div class="container">
        <h1>Python Execution Result</h1>
        <% if (pythonResult.error) { %>
            <p class="error">Error: <%= pythonResult.error %></p>
            <pre><%= pythonResult.output %></pre>
        <% } else { %>
            <% Object.keys(pythonResult).forEach(function (key) { %>
                <div class="card">
                    <h2>Paper <%= key %></h2>
                    <p><strong>Title:</strong> <%= pythonResult[key].title %></p>
                    <p><strong>Authors:</strong> <%= pythonResult[key].author_name.join(', ') %></p>
                    <p><strong>Author URLs:</strong></p>
                    <ul>
                        <% pythonResult[key].author_url.forEach(function (url) { %>
                            <li><a href="<%= url %>" target="_blank"><%= url %></a></li>
                        <% }); %>
                    </ul>
                    <p><strong>Target Author:</strong> <%= pythonResult[key].target_author.join(', ') %></p>
                    <p><strong>Conference:</strong> <%= pythonResult[key].conference %></p>
                    <p><strong>Year:</strong> <%= pythonResult[key].year %></p>
                    <p><strong>Source:</strong> <a href="<%= pythonResult[key].source %>" target="_blank"><%= pythonResult[key].source %></a></p>
                </div>
            <% }); %>
        <% } %>
    </div>
</body>
</html>
